{
  "name": "ionic-time-tracker",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "electron": "ionic cap build electron",
    "serve-chrome": "ionic serve --browser=chrome",
    "build:encrypter": "wasm-pack build -t web .\\static\\crypt_tasks\\ -d ./build && npm remove crypt_tasks && npm install ./static/crypt_tasks/build",
    "electron:mac": "electron-packager ./electron IonicTimeTracker --overwrite --platform=darwin --arch=arm64 --prune=true --out=release-builds",
    "electron:win": "electron-packager ./electron IonicTimetracker --overwrite --asar --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Ionic Time Tracker'",
    "electron:linux": "electron-packager ./electron IonicTimeTracker --overwrite --arch=x64 --platform=linux --prune=true --out=release-builds"
  },
  "dependencies": {
    "@capacitor-community/electron": "^5.0.1",
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.5.1",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@ionic/react": "^7.0.0",
    "@ionic/react-router": "^7.0.0",
    "@tanstack/react-query": "^5.14.1",
    "@tanstack/react-query-devtools": "^5.14.1",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "clsx": "^2.0.0",
    "crypt_tasks": "file:static/crypt_tasks/build",
    "dayjs": "^1.11.10",
    "formik": "^2.4.5",
    "ionicons": "^7.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@capacitor/cli": "5.5.1",
    "@tanstack/eslint-plugin-query": "^5.12.1",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "^13.5.0",
    "electron-packager": "^17.1.2",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^22.1.0",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  },
  "description": "An Ionic project"
}
